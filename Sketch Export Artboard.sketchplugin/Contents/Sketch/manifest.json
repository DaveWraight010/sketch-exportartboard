{
	"name": "Export Artboard",
	"description": "Plugin to Quickly export the selected Artboard to a PNG",
	"author": "Dave Wraight",
	"homepage": "https://github.com/DaveWraight010/sketch-exportartboard",
	"version": "1.1",
	"identifier": "au.com.010.sketch.sketch-exportartboard",
